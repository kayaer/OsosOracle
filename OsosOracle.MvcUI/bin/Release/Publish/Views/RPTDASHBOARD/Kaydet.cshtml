
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.RPTDASHBOARDModels.RPTDASHBOARDKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "RPTDASHBOARD", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.RPTDASHBOARD.KAYITNO)
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.DateInput(t => t.RPTDASHBOARD.TARIH, new { placeholder = _T("TARIH") })
            @Html.ValidationMessageFor(t => t.RPTDASHBOARD.TARIH)

        </div>

        <div class="form-group">
            @Html.TextBoxFor(t => t.RPTDASHBOARD.ADET, new { @class = "form-control int", placeholder = _T("ADET") })
            @Html.ValidationMessageFor(t => t.RPTDASHBOARD.ADET)

        </div>

        <div class="form-group">
            @Html.AutoComplete(t => t.RPTDASHBOARD.KURUMKAYITNO, Enums.AutocompleteFuction.KurumGetir, Enums.AutoCompleteType.List, _T("Kurum").ToString())
            @Html.ValidationMessageFor(t => t.RPTDASHBOARD.KURUMKAYITNO)

        </div>



        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
            @Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
		$(document).ready(function () {
		//js kodunuzu buraya yazın
		});

    </script>
}
