@using OsosOracle.MvcUI.Resources
@model OsosOracle.Entities.ComplexType.ENTABONEComplexTypes.AboneIslemleri


<div class="panel panel-default">
    <div class="panel-heading">@Dil.AboneBilgileri</div>
    <div class="panel-body">
        <div class="form-horizontal">
            <div class="form-group">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.AboneAdSoyad, new { @class = "form-control", placeholder = _T("Ad Soyad") })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.AboneNo, new { @class = "form-control", placeholder = _T("Abone No") })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.TarifeAdi, new { @class = "form-control", placeholder = _T("Tarife") })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.SonSatisTarihi, new { @class = "form-control", placeholder = _T("Son Satış Tarihi") })
                </div>
            </div>
            <div class="form-group hide">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.ABONEKAYITNO, new { @class = "form-control", placeholder = _T("Abone Kayıt No") })
                </div>
            </div>
            <div class="form-group hide">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.EntAboneSayacDetay.SAYACKAYITNO, new { @class = "form-control", placeholder = _T("Sayac Kayıt No") })
                </div>
            </div>
            <div class="form-group hide">
                <div class="col-md-12">
                    @Html.TextBoxFor(t => t.KalorimetreKayitNo, new { @class = "form-control", placeholder = _T("Kalorimetre Kayıt No") })
                </div>
            </div>
        </div>

    </div>
</div>
