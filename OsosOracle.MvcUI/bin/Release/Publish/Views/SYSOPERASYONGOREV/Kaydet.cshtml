
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.SYSOPERASYONGOREVModels.SYSOPERASYONGOREVKaydetModel

@section panelHeading
{
	
}

<div class="ajaxForm">

	@using (Html.BeginForm("Kaydet", "SYSOPERASYONGOREV", FormMethod.Post))
	{
		@Html.AntiForgeryToken()
		@Html.HiddenFor(t => t.SYSOPERASYONGOREV.KAYITNO)
		@Html.HiddenFor(t => t.SYSOPERASYONGOREV.VERSIYON)
		@Html.ValidationSummary(true)

	         <div class="form-group">
    @Html.TextBoxFor(t => t.SYSOPERASYONGOREV.OPERASYONKAYITNO, new { @class = "form-control int", placeholder = _T("OPERASYONKAYITNO") })
    @Html.ValidationMessageFor(t => t.SYSOPERASYONGOREV.OPERASYONKAYITNO)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.SYSOPERASYONGOREV.GOREVKAYITNO, new { @class = "form-control int", placeholder = _T("GOREVKAYITNO") })
    @Html.ValidationMessageFor(t => t.SYSOPERASYONGOREV.GOREVKAYITNO)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.SYSOPERASYONGOREV.VERSIYON, new { @class = "form-control int", placeholder = _T("VERSIYON") })
    @Html.ValidationMessageFor(t => t.SYSOPERASYONGOREV.VERSIYON)

    </div>


	 
		<div class="form-actions">
			<button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
			@Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
		</div>
	}
</div>

@section script
{
	<script type="text/javascript">
		$(document).ready(function () {
		//js kodunuzu buraya yazın	
		});

	</script>
}
