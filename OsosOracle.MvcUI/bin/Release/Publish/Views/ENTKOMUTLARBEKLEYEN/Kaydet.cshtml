
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.ENTKOMUTLARBEKLEYENModels.ENTKOMUTLARBEKLEYENKaydetModel

@section panelHeading
{
	
}

<div class="ajaxForm">

	@using (Html.BeginForm("Kaydet", "ENTKOMUTLARBEKLEYEN", FormMethod.Post))
	{
		@Html.AntiForgeryToken()
		@Html.HiddenFor(t => t.ENTKOMUTLARBEKLEYEN.KAYITNO)
		@Html.HiddenFor(t => t.ENTKOMUTLARBEKLEYEN.VERSIYON)
		@Html.ValidationSummary(true)

	         <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUTID, new { @class = "form-control int", placeholder = _T("KOMUTID") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUTID)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.BAGLANTIDENEMESAYISI, new { @class = "form-control int", placeholder = _T("BAGLANTIDENEMESAYISI") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.BAGLANTIDENEMESAYISI)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.ACIKLAMA, new { @class = "form-control int", placeholder = _T("ACIKLAMA") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.ACIKLAMA)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.GUIDID, new { @class = "form-control int", placeholder = _T("GUIDID") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.GUIDID)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.KONSSERINO, new { @class = "form-control int", placeholder = _T("KONSSERINO") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.KONSSERINO)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUTKODU, new { @class = "form-control int", placeholder = _T("KOMUTKODU") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUTKODU)

    </div>

       <div class="form-group">
    @Html.TextBoxFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUT, new { @class = "form-control int", placeholder = _T("KOMUT") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.KOMUT)

    </div>

       <div class="form-group">
    @Html.DateInput(t => t.ENTKOMUTLARBEKLEYEN.ISLEMTARIH, new { placeholder =  _T("ISLEMTARIH") })
    @Html.ValidationMessageFor(t => t.ENTKOMUTLARBEKLEYEN.ISLEMTARIH)

    </div>


	 
		<div class="form-actions">
			<button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
			@Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
		</div>
	}
</div>

@section script
{
	<script type="text/javascript">
		$(document).ready(function () {
		//js kodunuzu buraya yazın	
		});

	</script>
}
