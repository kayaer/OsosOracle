
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.PRMTARIFEELKModels.PRMTARIFEELKKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "PRMTARIFEELK", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.PRMTARIFEELK.KAYITNO)
        @Html.HiddenFor(t => t.PRMTARIFEELK.VERSIYON)
        @Html.HiddenFor(t=>t.PRMTARIFEELK.OLUSTURAN)
        @Html.HiddenFor(t=>t.PRMTARIFEELK.GUNCELLEYEN)
        @Html.ValidationSummary(true)

        <div class="form-group row">
            <div class="col-md-6">
                @Html.AutoComplete(t => t.PRMTARIFEELK.KURUMKAYITNO, Enums.AutocompleteFuction.KurumGetir, Enums.AutoCompleteType.List, _T("Kurum").ToString())
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.KURUMKAYITNO)
            </div>
            <div class="col-md-6">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.AD, new { @class = "form-control", placeholder = _T("Tarife Adı") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.AD)
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.KREDIKATSAYI, new { @class = "form-control int", placeholder = _T("Kredi Katsayı") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.KREDIKATSAYI)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.YEDEKKREDI, new { @class = "form-control int", placeholder = _T("Yedek Kredi") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.YEDEKKREDI)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.KRITIKKREDI, new { @class = "form-control int", placeholder = _T("Kritik Kredi") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.KRITIKKREDI)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.CARPAN, new { @class = "form-control int", placeholder = _T("Çarpan") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.CARPAN)
            </div>
        </div>
        <div class="form-group row">
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.FIYAT1, new { @class = "form-control decimal", placeholder = _T("Fiyat1") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.FIYAT1)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.FIYAT2, new { @class = "form-control decimal", placeholder = _T("Fiyat2") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.FIYAT2)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.FIYAT3, new { @class = "form-control decimal", placeholder = _T("Fiyat3") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.FIYAT3)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.YUKLEMELIMIT, new { @class = "form-control int", placeholder = _T("Yükleme Limiti") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.YUKLEMELIMIT)
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.LIMIT1, new { @class = "form-control decimal", placeholder = _T("Limit 1") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.LIMIT1)

            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.LIMIT2, new { @class = "form-control decimal", placeholder = _T("Limit 2") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.LIMIT2)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.SABAHSAAT, new { @class = "form-control", placeholder = _T("Sabah Saat") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.SABAHSAAT)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.AKSAMSAAT, new { @class = "form-control", placeholder = _T("Akşam Saat") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.AKSAMSAAT)             
            </div>
        </div>
        <div class="form-group row">            
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.HAFTASONUAKSAM, new { @class = "form-control", placeholder = _T("Hafta Sonu Akşam") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.HAFTASONUAKSAM)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.SABITUCRET, new { @class = "form-control int", placeholder = _T("Sabit Ücret") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.SABITUCRET)
            </div>
            <div class="col-md-3">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.DURUM, new { @class = "form-control int", placeholder = _T("Durum") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.DURUM)
            </div>
        </div>

        <div class="form-group row">
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM1GUN, new {@class = "form-control int", placeholder = _T("Bayram 1 Gün")})
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM1GUN)
            </div>
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM1AY, new { @class = "form-control int", placeholder = _T("Bayram 1 Ay") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM1AY)
            </div>
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM1SURE, new { @class = "form-control int", placeholder = _T("Bayram 1 Süre") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM1SURE)
            </div>
           

        </div>
       
        <div class="form-group row">
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM2GUN, new {@class = "form-control int", placeholder = _T("Bayram 2 Gün")})
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM2GUN)
            </div>
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM2AY, new { @class = "form-control int", placeholder = _T("Bayram 2 Ay") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM2AY)
            </div>
            <div class="col-md-4">
                @Html.TextBoxFor(t => t.PRMTARIFEELK.BAYRAM2SURE, new { @class = "form-control int", placeholder = _T("Bayram 2 Süre") })
                @Html.ValidationMessageFor(t => t.PRMTARIFEELK.BAYRAM2SURE)
            </div>       

        </div>
        <div class="form-group">
            @Html.TextAreaFor(t => t.PRMTARIFEELK.ACIKLAMA, new { @class = "form-control int", placeholder = _T("Açıklama") })
            @Html.ValidationMessageFor(t => t.PRMTARIFEELK.ACIKLAMA)
        </div>

     



        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
            @Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
            //js kodunuzu buraya yazın
        });

    </script>
}
