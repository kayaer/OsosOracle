
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@using OsosOracle.MvcUI.Resources
@model OsosOracle.MvcUI.Models.SYSROLKULLANICIModels.SYSROLKULLANICIKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "SYSROLKULLANICI", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.SYSROLKULLANICI.KAYITNO)
        @Html.HiddenFor(t => t.SYSROLKULLANICI.VERSIYON)
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.AutoComplete(t => t.SYSROLKULLANICI.KULLANICIKAYITNO, Enums.AutocompleteFuction.KullaniciGetir, Enums.AutoCompleteType.List,Dil.Kullanici)
            @Html.ValidationMessageFor(t => t.SYSROLKULLANICI.KULLANICIKAYITNO)

        </div>

        <div class="form-group">
            @Html.AutoComplete(t => t.SYSROLKULLANICI.ROLKAYITNO, Enums.AutocompleteFuction.RolGetir, Enums.AutoCompleteType.List,Dil.Rol)
            @Html.ValidationMessageFor(t => t.SYSROLKULLANICI.ROLKAYITNO)

        </div>


        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @Dil.Kaydet</button>
            @Html.ButtonLink(Dil.Vazgec, "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
