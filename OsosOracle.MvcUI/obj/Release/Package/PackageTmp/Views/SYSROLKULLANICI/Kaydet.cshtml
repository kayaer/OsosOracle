
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.SYSROLKULLANICIModels.SYSROLKULLANICIKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "SYSROLKULLANICI", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.SYSROLKULLANICI.KAYITNO)
        @Html.HiddenFor(t => t.SYSROLKULLANICI.VERSIYON)
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.AutoComplete(t => t.SYSROLKULLANICI.KULLANICIKAYITNO, Enums.AutocompleteFuction.KullaniciGetir, Enums.AutoCompleteType.List, _T("Kullanıcı").ToString())
            @Html.ValidationMessageFor(t => t.SYSROLKULLANICI.KULLANICIKAYITNO)

        </div>

        <div class="form-group">
            @Html.AutoComplete(t => t.SYSROLKULLANICI.ROLKAYITNO, Enums.AutocompleteFuction.RolGetir, Enums.AutoCompleteType.List, _T("Rol").ToString())
            @Html.ValidationMessageFor(t => t.SYSROLKULLANICI.ROLKAYITNO)

        </div>


        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
            @Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
            //js kodunuzu buraya yazın
        });

    </script>
}
