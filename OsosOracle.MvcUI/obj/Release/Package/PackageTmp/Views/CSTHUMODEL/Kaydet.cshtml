
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.CSTHUMODELModels.CSTHUMODELKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "CSTHUMODEL", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t=>t.CSTHUMODEL.OLUSTURAN)
        @Html.HiddenFor(t => t.CSTHUMODEL.KAYITNO)
        @Html.HiddenFor(t => t.CSTHUMODEL.VERSIYON)
        @Html.ValidationSummary(true)


    <div class="form-group">
        @Html.AutoComplete(t => t.CSTHUMODEL.MARKAKAYITNO, Enums.AutocompleteFuction.ModemMarkaGetir, Enums.AutoCompleteType.List, _T("Modem Marka").ToString())

        @Html.ValidationMessageFor(t => t.CSTHUMODEL.MARKAKAYITNO)

    </div>
        <div class="form-group">
            @Html.TextBoxFor(t => t.CSTHUMODEL.AD, new { @class = "form-control", placeholder = _T("Ad") })
            @Html.ValidationMessageFor(t => t.CSTHUMODEL.AD)

        </div>
      
        <div class="form-group">
            @Html.TextAreaFor(t => t.CSTHUMODEL.ACIKLAMA, new { @class = "form-control int", placeholder = _T("Açıklama") })
            @Html.ValidationMessageFor(t => t.CSTHUMODEL.ACIKLAMA)

        </div>


        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
            @Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
		$(document).ready(function () {
		//js kodunuzu buraya yazın
		});

    </script>
}
