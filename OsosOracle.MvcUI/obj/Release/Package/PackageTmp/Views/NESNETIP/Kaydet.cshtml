
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.NESNETIPModels.NESNETIPKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "NESNETIP", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.NESNETIP.KAYITNO)
        @Html.HiddenFor(t => t.NESNETIP.VERSIYON)
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.TextBoxFor(t => t.NESNETIP.ADI, new { @class = "form-control", placeholder = _T("Nesne Tip Adı") })
            @Html.ValidationMessageFor(t => t.NESNETIP.ADI)

        </div>



        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @_T("Kaydet")</button>
            @Html.ButtonLink(_T("Vazgeç").ToString(), "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
            //js kodunuzu buraya yazın
        });

    </script>
}
