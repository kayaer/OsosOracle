@using OsosOracle.MvcUI.Resources
@using OsosOracle.MvcUI.Infrastructure
@using OsosOracle.Framework.HtmlHelpers
@model OsosOracle.MvcUI.Models.SYSROLModels.SYSROLKaydetModel

@section panelHeading
{

}

<div class="ajaxForm">

    @using (Html.BeginForm("Kaydet", "SYSROL", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        @Html.HiddenFor(t => t.SYSROL.KAYITNO)
        @Html.HiddenFor(t => t.SYSROL.VERSIYON)
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.TextBoxFor(t => t.SYSROL.AD, new { @class = "form-control", placeholder = Dil.Rol })
            @Html.ValidationMessageFor(t => t.SYSROL.AD)

        </div>

        <div class="form-group">
            @Html.TextBoxFor(t => t.SYSROL.ACIKLAMA, new { @class = "form-control", placeholder = Dil.Aciklama })
            @Html.ValidationMessageFor(t => t.SYSROL.ACIKLAMA)

        </div>



        <div class="form-group">
            @Html.AutoComplete(t => t.SYSROL.KURUMKAYITNO, Enums.AutocompleteFuction.KurumGetir, Enums.AutoCompleteType.List, Dil.Kurum)
            @Html.ValidationMessageFor(t => t.SYSROL.KURUMKAYITNO)
        </div>



        <div class="form-actions">
            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> @Dil.Kaydet</button>
            @Html.ButtonLink(Dil.Vazgec, "Index", "btn btn-xs btn-danger cancel", "fa-remove")
        </div>
    }
</div>

@section script
{
    <script type="text/javascript">
        $(document).ready(function () {
            //js kodunuzu buraya yazÄ±n
        });

    </script>
}
